FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changePatchTypeDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    T
    {
        internalField   uniform 300;

        boundaryField
        {
            wall
            {
                type            fixedValue;
                value           uniform 400;
            }

            LIB_to_channel
            {
                type            solidWallMixedTemperatureCoupled;
                neighbourRegionName channel;
                neighbourPatchName channel_to_LIB;
                neighbourFieldName T;
                Kappa           Kappa;
                value           uniform 300;
            }
        }
    }

    rho
    {
        internalField   uniform 8000;

        boundaryField
        {
            wall
            {
                type            zeroGradient;
            }

            LIB_to_channel
            {
                type            zeroGradient;
            }
        }
    }

    Kappa
    {
        internalField   uniform 80;

        boundaryField
        {
            wall
            {
                type            zeroGradient;
            }

            LIB_to_channel
            {
                type            zeroGradient;
            }
        }
    }

    cp
    {
        internalField   uniform 450;

        boundaryField
        {
            wall
            {
                type            zeroGradient;
            }

            LIB_to_channel
            {
                type            zeroGradient;
            }
        }
    }
}


